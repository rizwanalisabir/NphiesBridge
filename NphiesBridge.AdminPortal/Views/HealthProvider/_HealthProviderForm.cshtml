@model NphiesBridge.Core.Entities.HealthProvider

<div class="form-group">
    <label asp-for="Name" class="form-label">Name</label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger small"></span>
</div>

<div class="form-group">
    <label asp-for="LicenseNumber" class="form-label">License Number</label>
    <input asp-for="LicenseNumber" class="form-control" />
    <span asp-validation-for="LicenseNumber" class="text-danger small"></span>
</div>

<div class="form-group">
    <label asp-for="Email" class="form-label">Email</label>
    <input asp-for="Email" class="form-control" />
    <span asp-validation-for="Email" class="text-danger small"></span>
</div>

<div class="form-group">
    <label asp-for="Phone" class="form-label">Phone</label>
    <input asp-for="Phone" class="form-control" />
    <span asp-validation-for="Phone" class="text-danger small"></span>
</div>

<div class="form-check mt-2">
    <input asp-for="IsActive" class="form-check-input" />
    <label asp-for="IsActive" class="form-check-label">Is Active</label>
</div>
